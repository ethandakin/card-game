{
  "name": "Rolatro",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/modules/public",
      "$ignoreUnknownInstances": true, 

      "Packages": {
        "$path": "Packages",
        "$ignoreUnknownInstances": true
      },

      "Remote": {
        "$ignoreUnknownInstances": true,
        "$className": "RemoteEvent"
      }
    },

    "ServerStorage": {
      "$path": "src/modules/private",
      "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "$ignoreUnknownInstances": true,
      "$path": "src/server"
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
          "$ignoreUnknownInstances": true,
          "$path": "src/client"
      }
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}